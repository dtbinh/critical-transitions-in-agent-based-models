file = {:C$\backslash$:/Users/Kyle Shank/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bettencourt et al. - 2010 - Urban scaling and its deviations revealing the structure of wealth, innovation and crime across cities.pdf:pdf},
