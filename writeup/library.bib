journal = {Nature},
keywords = {Biomass,Cyanobacteria,Cyanobacteria: growth \& development,Cyanobacteria: radiation effects,Feedback,Feedback: radiation effects,Kinetics,Light,Models, Biological,Photosynthesis,Photosynthesis: radiation effects,Stochastic Processes},
month = jan,
