file = {:C$\backslash$:/Users/Kyle Shank/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Walters, Maguire - 1996 - Lessons for stock assessment from the northern cod collapse.pdf:pdf},
